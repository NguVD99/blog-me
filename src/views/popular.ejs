<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Popular</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="/css/base.css">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/fonts/themify-icons/themify-icons.css" </head>

<body>
    <%- include('./partials/header') %>
        <div id="main-container" class="container">
            <div class="grid">
                <div class="grid__row">
                    <% listUsers.forEach(function(information) { %>
                        <div class="grid__column-3">
                            <a class="container-item" href="<%= information.linkDocument %>">
                                <div class="bg-category">
                                    <h2 class="bg-category-name">Kinh nghiệm lập trình</h2>
                                </div>
                                <!-- <div class="bg-container"> -->
                                <img class="container-item-img" src="<%= information.image %>" alt="">

                                <h2 class="container-item-name">
                                    <%= information.nameInformation %>
                                </h2>
                                <div class="container-item-footer">
                                    <div class="author">
                                        <img class="avatar-img" src="/images/avatar.png" alt="">
                                        <div class="name-avatar">Vũ Duy Ngự</div>
                                    </div>
                                    <div class="item-footer-like">
                                        <i class="ti-heart"></i>
                                    </div>
                                </div>

                                <!-- </div> -->
                            </a>
                        </div>
                        <% }); %>
                </div>
            </div>
        </div>

        <%- include('./partials/footer') %>


            <div class="modal">
                <div class="modal-overlay">

                </div>
            </div>

</body>

</html>